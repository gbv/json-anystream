  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-gbv": "github:gbv/eslint-config-gbv",
    "form-data": "^3.0.0",
    "mocha": "^8.1.3",
    "mocha-eslint": "^6.0.0",
    "nock": "^13.0.4",
    "pre-commit": "^1.2.2"
  },
